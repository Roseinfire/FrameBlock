{
  "manifest_version": 3,
  "name": "FrameBlock",
  "version": "1.2.2",
  "description": "Protection from frames, banners and shadow content.",
  "content_scripts": [
    {
          "matches": ["<all_urls>"],
          "js": ["frameblock.js"]
        }
    ],
  "icons": {
    "48": "Frameblock(48x48).png",
    "128": "Frameblock(128x128).png"
    }
}
